[
  {
    "name": "composer",
    "columns": [
      {
        "header": "#",
        "extractor": "index",
        "width": "3%",
        "justification": "right"
      },
      {
        "header": "Composer",
        "extractor": "(composer join ' & ') or composerDetail.name",
        "width": "14%"
      },
      {
        "header": "Born",
        "extractor": "shortDate composerDetail.born",
        "width": "5.6%",
        "justification": "center"
      },
      {
        "header": "Died",
        "extractor": "shortDate composerDetail.died",
        "width": "5.6%",
        "justification": "center"
      },
      {
        "header": "Title",
        "extractor": "title",
        "width": "25%"
      },
      {
        "header": "Artist",
        "extractor": "artists",
        "width": "10%"
      },
      {
        "header": "Album",
        "extractor": "album",
        "width": "18%"
      },
      {
        "header": "Disk",
        "extractor": "(disk || '') + '/' + (nDisks || '')",
        "width": "3%",
        "justification": "center"
      },
      {
        "header": "Track",
        "extractor": "(track || '') + '/' + (nTracks || '')",
        "width": "4%",
        "justification": "center"
      },
      {
        "header": "Time",
        "extractor": "shortDur duration",
        "width": "3%",
        "justification": "right"
      },
      {
        "header": "Plays",
        "extractor": "plays || '0'",
        "width": "3%",
        "justification": "right"
      },
      {
        "header": "Last played",
        "extractor": "shortTime lastPlayed",
        "width": "6%",
        "justification": "right"
      }
    ],
    "separator": "â”‚",
    "hdrTheming": {
      "chalk": [
        "italic",
        "underline",
        "blueBright"
      ]
    },
    "separatorTheming": {
      "chalk": [
        "gray"
      ]
    },
    "hdrSeparatorTheming": {
      "chalk": [
        "gray"
      ]
    }
  }
]
